{
  "routes": [
    {
      "route": "/callback.html",
      "serve": "/callback.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/callback.html"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".html": "text/html"
  }
}
